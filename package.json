{
  "name": "boilerplate-express-craft",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "migrate": "yarn sequelize-cli db:migrate",
    "migration:create": "yarn sequelize-cli migration:create --underscored",
    "model:create": "yarn sequelize-cli model:create --underscored",
    "seed:create": "yarn sequelize-cli seed:generate",
    "seed": "yarn sequelize-cli db:seed:all",
    "generate:key": "cd ./src/config/secret;openssl ecparam -name prime256v1 -genkey -noout -out private_key.pem; openssl ec -in private_key.pem -pubout -out public_key.pem",
    "prepare": "husky",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "bin": {
    "crudify": "node_modules/@dctrl/crudify/index.js"
  },
  "author": "Reclone vanhan",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^5.7.1",
    "@bull-board/express": "^5.7.1",
    "@dctrl/crudify": "^1.0.5",
    "aws-sdk": "^2.1429.0",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.2.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "helmet": "^7.0.0",
    "html-entities": "^2.6.0",
    "html-to-text": "^9.0.5",
    "http-status": "^2.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.1",
    "juice": "^9.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.1",
    "sequelize": "^6.32.1",
