    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.3.1",
    "eslint-plugin-security": "^3.0.1",
    "faker": "^6.6.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
